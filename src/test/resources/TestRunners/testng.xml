<?xml version="1.0" encoding="UTF-8"?>
<!DOCTYPE suite SYSTEM "http://testng.org/testng-1.0.dtd">

<suite name="HRMS">

	<listeners>
		<listener class-name="com.hrms.listeners.ExtentReportListeners"></listener>
	</listeners>


	<test name="HRMS automation masters" thread-count="5" parallel="classes">
		<groups>
			<run>
				<include name="Regression"></include>
			</run>

		</groups>


		<classes>

			<class name="com.hrms.masters.tests.MastersGeographyTest" />

			<class name="com.hrms.masters.tests.MastersPersonalTest" />

			 	<class name="com.hrms.masters.tests.MastersEducationTest" />
		
			<class name="com.hrms.masters.tests.MastersPositionTest" />
		</classes>

	</test>


</suite>